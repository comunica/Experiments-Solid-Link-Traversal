      "cpu_percentage": 100
    },
    "configClient": "input/config-client/call-none.json",
    "contextClient": "input/context-client.json",
    "clientPort": 3001,
    "clientLogLevel": "info",
    "queryTimeout": 300,
    "maxMemory": 8192
  }
